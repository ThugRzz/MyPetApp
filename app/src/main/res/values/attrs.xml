<?xml version="1.0" encoding="utf-8"?>
<resources>

    <item name="icon" type="drawable" />

    <declare-styleable name="RoundedButton">
        <attr name="roundedButtonBgColor" format="color" />
        <attr name="roundedButtonText" format="string" />
        <attr name="roundedButtonForeground" format="reference" />
        <attr name="roundedButtonTextAppearance" format="reference" />
        <attr name="roundedButtonRadius" format="dimension" />
    </declare-styleable>

    <declare-styleable name="IndicatorsView">
        <attr name="indicatorsColorCurrent" format="color" />
        <attr name="indicatorsColorDefault" format="color" />
        <attr name="indicatorsMargin" format="dimension" />
        <attr name="indicatorsRadius" format="dimension" />
        <attr name="indicatorsTotalCount" format="integer" />
        <attr name="indicatorsCurrentPosition" format="integer" />
    </declare-styleable>

    <declare-styleable name="ShadowIconView">
        <attr name="shadowIcon" format="reference" />
        <attr name="shadowIconTint" format="color" />
    </declare-styleable>

    <declare-styleable name="TitleBarView">
        <attr name="titleText" format="string" />
        <attr name="titleStartIcon" format="reference" />
        <attr name="titleEndIcon" format="reference" />
    </declare-styleable>

    <declare-styleable name="ProfileParametersView">
        <attr name="leftTitle" format="string" />
        <attr name="rightTitle" format="string" />
        <attr name="icon" format="reference" />
    </declare-styleable>

    <declare-styleable name="ReferenceItemView">
        <attr name="itemDrawable" format="reference" />
        <attr name="itemTitle" format="string" />
    </declare-styleable>

    <declare-styleable name="ProfileItemView">
        <attr name="profileItemActionIcon" format="reference" />
        <attr name="profileItemIcon" format="reference" />
        <attr name="profileItemIconBgColor" format="color" />
        <attr name="profileItemTopText" format="string" />
        <attr name="profileItemMiddleText" format="string" />
        <attr name="profileItemBottomText" format="string" />
    </declare-styleable>


</resources>